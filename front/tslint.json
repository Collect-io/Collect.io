{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "indent": [
      true
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "arrow-parens": false,
    "semicolon": [
      true,
      "never"
    ],
    "object-literal-sort-keys": false
  },
  "rulesDirectory": [],
  "exclude": [
    "node_modules"
  ]
}
